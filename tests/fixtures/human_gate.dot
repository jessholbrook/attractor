digraph Review {
    rankdir=LR

    start [shape=Mdiamond]
    exit  [shape=Msquare]

    review_gate [shape=hexagon, label="Review Changes"]

    ship_it [label="Ship It", prompt="Deploy"]
    fixes   [label="Fix Issues", prompt="Fix the issues"]

    start -> review_gate
    review_gate -> ship_it [label="[A] Approve"]
    review_gate -> fixes   [label="[F] Fix"]
    ship_it -> exit
    fixes -> review_gate
}
